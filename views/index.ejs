<% include ./partials/header %>

<h1>My Index</h1>
<% blogs.forEach(function(blog){ %>
    <article>
        <h2><%= blog.title %></h2>
        <p><em><%= blog.date %></em></p>
        <img src="<%= blog.image %>">
        <p><%= blog.text %></p>
    </article>
<% }) %>

<% include ./partials/footer %>